                    </select>
                    <span class="input-group-btn">
                    <button id="sendbtn"  onclick="send()" class="btn btn-success btn-block"><i id="send" class=""></i>Get</button>
                  </span>
                </div><!-- /input-group -->
                </div>

                <div class="col-lg-2 col-md-2">
                        <a href="/dashboardm" class="btn btn-info btn-block">><i class="fa fa-lightbulb-o"></i>Miladi Dashboard</a>
                </div>
            </div>
