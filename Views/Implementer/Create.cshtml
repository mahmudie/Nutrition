                </div>
                <label asp-for="ImpAcronym" class="col-md-2 control-label"></label>
                <div class="col-md-3">
                    <input asp-for="ImpAcronym" class="form-control" />
                    <span asp-validation-for="ImpAcronym" class="text-danger" />
                </div>
            </div>
            <div class="form-group">

                <label asp-for="ImpName" class="col-md-2 control-label"></label>
                <div class="col-md-3">
                    <input asp-for="ImpName" class="form-control" />
                    <span asp-validation-for="ImpName" class="text-danger" />
                </div>
                <label asp-for="ImpNameDari" class="col-md-2 control-label"></label>
                <div class="col-md-3">
                    <input asp-for="ImpNameDari" class="form-control" />
                    <span asp-validation-for="ImpNameDari" class="text-danger" />
                </div>
            </div>

            <div class="form-group">

                <label asp-for="ImpNamePashto" class="col-md-2 control-label"></label>
                <div class="col-md-3">
                    <input asp-for="ImpNamePashto" class="form-control" />
                    <span asp-validation-for="ImpNamePashto" class="text-danger" />
                </div>
                <label asp-for="RegistrationDate" class="col-md-2 control-label"></label>
                <div class="col-md-3">
                    <input asp-for="RegistrationDate" class="form-control" />
                    <span asp-validation-for="RegistrationDate" class="text-danger" />
                </div>
            </div>
            <div class="form-group">

                <label asp-for="OtherAddress" class="col-md-2 control-label"></label>
                <div class="col-md-3">
                    <input asp-for="OtherAddress" class="form-control" />
                    <span asp-validation-for="OtherAddress" class="text-danger" />
                </div>
