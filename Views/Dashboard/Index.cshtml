@{
    Layout=null;
}
    <title>Dashboard</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/logo.ico" />
    <link href="~/Dash/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Dash/css/components.min.css" rel="stylesheet" />
