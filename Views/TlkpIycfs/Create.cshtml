@model DataSystem.Models.TlkpIycf

@{
    ViewData["Title"] = "Create";
}
<div class="container">
    <h2>Create</h2>

    <form asp-action="Create">
        <div class="form-horizontal">
            <hr />
            <div class="form-group">
                <label asp-for="CauseShortName" class="col-md-2 control-label"></label>
                <div class="col-md-5">
                    <input asp-for="CauseShortName" class="form-control" />
